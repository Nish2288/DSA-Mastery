   "source": [
    "# Check if list is sorted or not"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "def check_sorted(l):\n",
    "    if len(l) == 1 or len(l) == 0:\n",
    "        return True\n",
    "    if l[0]>l[1]:\n",
    "        return False\n",
    "    \n",
    "    return check_sorted(l[1:])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [],
   "source": [
    "l = [2,3,2,5]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "False"
      ]
     },
     "execution_count": 14,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "check_sorted(l)"
   ]
