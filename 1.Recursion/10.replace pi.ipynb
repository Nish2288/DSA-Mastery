   "metadata": {},
   "outputs": [],
   "source": [
    "def replacePI(str):\n",
    "    if len(str) == 0:\n",
    "        return str\n",
    "    \n",
    "    if str[0] == \"p\" and str[1] == \"i\":\n",
    "        return replacePI(str[2:])\n",
    "    else:\n",
    "        return str[0]+replacePI(str[1:])"
   ]
