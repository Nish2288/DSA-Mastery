    "def firstIndex(a, n, idx):\n",
    "    if len(a) == idx :\n",
    "        return -1\n",
    "    \n",
    "    if a[idx] == n :\n",
    "        return idx\n",
    "    \n",
    "    return firstIndex(a,n,idx+1)"
   ]
