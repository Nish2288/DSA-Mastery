   "source": [
    "def palindrome(s,si,ei):\n",
    "    if si>ei:\n",
    "        return True\n",
    "    \n",
    "    if s[si] == s[ei]:\n",
    "        return palindrome(s, si+11, ei-1)\n",
    "    else:\n",
    "        return False\n",
    "    "
   ]
  },
